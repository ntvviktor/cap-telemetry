{
  "name": "learning-dynatrace",
  "version": "1.0.0",
  "description": "CAP TypeScript Bookshop Example",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node src/main.js",
    "dev": "tsc --watch",
    "watch": "cds watch --profile development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@cap-js/cds-types": "^0.15.0",
    "@cap-js/sqlite": "^2.1.0",
    "@cap-js/telemetry": "^1.5.4",
    "@sap/cds": "^9.4.4",
    "express": "^4.18.2"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "sqlite3"
        }
      },
      "telemetry": {
        "kind": "to-console",
        "format": "text"
      }
    }
  }
}
